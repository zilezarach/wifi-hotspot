generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id         Int      @id @default(autoincrement())
  mac        String
  ip         String
  planName   String
  dataCap    Int?
  planHours  Int
  checkoutRequestId  String?
  expiry     DateTime
  paid       Boolean  @default(false)
}
